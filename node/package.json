{
  "name": "subnet-111",
  "version": "1.1.0",
  "type": "module",
  "description": "Bittensor Subnet 111 - A decentralized protocol for open-source intelligence",
  "main": "miner.js",
  "imports": {
    "#modules/*": "./modules/*",
    "#routes/*": "./routes/*",
    "#utils/*": "./utils/*",
    "#config": "./config.js"
  },
  "scripts": {
    "miner:start": "node miner.js",
    "miner:dev": "nodemon miner.js",
    "validator:start": "node validator.js",
    "validator:dev": "nodemon validator.js",
    "test": "jest --coverage",
    "lint": "eslint .",
    "prepare": "husky"
  },
  "dependencies": {
    "apify-client": "^2.7.1",
    "chalk": "^5.4.1",
    "country-state-city": "^3.2.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "i": "^0.3.7",
    "node-fetch": "^3.3.2",
    "npm": "^11.4.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.27.2",
    "eslint": "^9.28.0",
    "eslint-plugin-jest": "^28.12.0",
    "eslint-plugin-unicorn": "^59.0.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "bittensor",
    "subnet",
    "miner"
  ],
  "jest": {
    "verbose": true,
    "restoreMocks": true,
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
      }
    },
    "collectCoverageFrom": [
      "modules/**/*.js",
      "routes/**/*.js",
      "utils/**/*.js",
      "!node_modules/**",
      "!coverage/**",
      "!jest.config.js",
      "!eslint.config.js"
    ],
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "moduleNameMapper": {
      "^#modules/(.*)$": "<rootDir>/modules/$1",
      "^#routes/(.*)$": "<rootDir>/routes/$1",
      "^#utils/(.*)$": "<rootDir>/utils/$1",
      "^#config$": "<rootDir>/config.js"
    }
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "author": "oneoneone",
  "license": "MIT"
}
